CC = avr32-linux-gcc
CFLAGS = -Wall -O2
CPPFLAGS = -I..
LFLAGS = -static

AR = avr32-linux-ar
ifeq ($(OS),SunOS)
   AR = /usr/ccs/bin/ar
endif
   

OS = $(shell uname)

#ARCH = $(shell uname -m)
ARCH=avr32

LIBSYSINFO = libsysinfo-0.2.0

PREFIX = /usr/local/


INSTALL= /usr/bin/install -c

INSTALL_BINPATH    = $(PREFIX)/bin
INSTALL_MANPATH    = $(PREFIX)/share/man
INSTALL_DOCPATH    = $(PREFIX)/share/doc
INSTALL_LOCALEPATH = $(PREFIX)/share/locale
PACKAGE = linux_logo
