diff -Nur linux_logo-3.9b1/po/Makefile linux_logo-3.9b1-i18n/po/Makefile
--- linux_logo-3.9b1/po/Makefile	Wed Dec 31 21:00:00 1969
+++ linux_logo-3.9b1-i18n/po/Makefile	Mon Oct 23 20:24:31 2000
@@ -0,0 +1,31 @@
+INSTALL= /usr/bin/install -c
+INSTALL_CMD= ${INSTALL} -m 644
+INSTALLDIR=/usr/share/locale
+PACKAGE = linux_logo
+
+CATALOGS = pt_BR.mo
+
+POTFILES= ../*.c ../*.h
+
+all: $(PACKAGE).pot $(CATALOGS)
+
+$(PACKAGE).pot: $(POTFILES)
+	xgettext --default-domain=$(PACKAGE) --add-comments --keyword=_ --keyword=S_ $(POTFILES)
+	if cmp -s $(PACKAGE).po $(PACKAGE).pot; then \
+	    rm -f $(PACKAGE).po; \
+	else \
+	    mv $(PACKAGE).po $(PACKAGE).pot; \
+	fi
+
+clean:
+	rm -f *mo $(PACKAGE).pot
+
+install: $(CATALOGS)
+	for n in $(CATALOGS); do \
+	    l=`basename $$n .mo`; \
+	    $(INSTALL) -m 755 -d $(INSTALLDIR)/$$l; \
+	    $(INSTALL) -m 755 -d $(INSTALLDIR)/$$l/LC_MESSAGES; \
+	    $(INSTALL) -m 644 $$n $(INSTALLDIR)/$$l/LC_MESSAGES/$(PACKAGE).mo; \
+	done
+%.mo: %.po
+	msgfmt -o $@ $<
